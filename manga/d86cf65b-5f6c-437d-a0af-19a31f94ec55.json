   "jp": "Ijiranaide, Nagatoro-san"
  },
  "altTitles": [
   {
    "en": "Don't Toy With Me, Miss Nagatoro"
   },
   {
    "id": "Jangan goda aku, Nagatoro"
   },
   {
    "es": "No te burles de mi, Nagatoro"
   },
   {
    "en": "Please Don't Bully Me, Nagatoro"
   },
   {
    "fr": "S'il te plaît ne me harcèle pas, Nagatoro"
   },
   {
    "ru": "Не издевайся, Нагаторо-сан"
