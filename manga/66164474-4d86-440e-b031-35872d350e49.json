  "altTitles": [
   {
    "en": "Cô bạn bàn bên Satou-san"
   },
   {
    "en": "Satou-san Who Sits Next To Me"
   },
   {
    "en": "Satou-san, Quien Se Sienta a Mi Lado"
   },
   {
    "en": "Tôi thích một cô gái, nhưng chẳng dám ngỏ lời"
   },
   {
    "en": "Сато-сан, сидящая рядом со мной"
   },
   {
    "en": "隣の席の佐藤さん"
   }
